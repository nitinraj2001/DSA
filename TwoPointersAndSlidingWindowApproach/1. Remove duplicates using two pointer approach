class Solution {
    public int removeDuplicates(int[] nums) {
        int slow=0;
        for(int fast = 1; fast<nums.length;fast++){
            if(nums[fast]!=nums[slow]){
                slow++;
                nums[slow] = nums[fast]; // To maintain unquiue element array
            }
        }
        return slow+1; // here number of unique elements is equal to number of times slow pointer moves next + 1
    }
}
